{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 80,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": [
    "^@(.*)$",
    "^@components/(.*)$",
    "^@layout/(.*)$",
    "^@pages/(.*)$",
    "^@assets/(.*)$",
    "^@constants/(.*)$",
    "^@hooks/(.*)$",
    "^@state/(.*)$",
    "^@services/(.*)$",
    "^@router/(.*)$",
    "^@config/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}